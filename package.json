{
  "name": "decimalish",
  "version": "1.0.0",
  "license": "MIT",
  "description": "",
  "main": "decimalish.js",
  "module": "decimalish.mjs",
  "types": "decimalish.d.ts",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "test": "jest",
    "build": "npm-run-all clean -p build:*",
    "clean": "rm -rf out; mkdir -p out",
    "build:packagejson": "node -e \"var o={},p=require('./package.json'),k;for(k in p){if(k==='private')break;o[k]=p[k]}console.log(JSON.stringify(o,null,2))\" > out/package.json",
    "build:main": "rollup decimalish.ts -o out/decimalish.js -f cjs -p typescript -p buble -p terser",
    "build:module": "rollup decimalish.ts -o out/decimalish.mjs -f esm -p typescript",
    "build:types": "tsc decimalish.ts --outFile out/decimlaish.d.ts --declaration --emitDeclarationOnly"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.0.2",
    "big.js": "^6.1.1",
    "decimal": "^0.0.2",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.60.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "auto"
        }
      ],
      "@babel/preset-typescript"
    ]
  }
}
